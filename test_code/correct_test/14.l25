program MyApp { 
    func add(a, b) {
        let sum = a + b;
        return sum;
    } 
    func square(x) { 
        let result = x * x;
        return result; 
    } 
    main {
        let x = 5;
        let y = add(x, 10);
        let z = square(y); 
        if (z > 50) {
            output(z); 
        } else {
            output(0); 
        };
    }
}