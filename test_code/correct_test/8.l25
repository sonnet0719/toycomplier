program TestScope{
    func add(a, b){
        let res = a + b;
        return res;
    }
    
    main{
        let res = 0;
        if(res == 0){
            let x = add(1, 2);
            while(x < 5){
                x = x + 1;
                let y = x;
                res = y;
                output(x, y, res);
            };
        }
        else{
            let x = add(3, 4);
            res = x;
        };
        output(res);
    }
}